<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/comp_magnet_ll_btns"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <EditText
        android:id="@+id/et_keyword"
        android:layout_width="match_parent"
        android:layout_height="@dimen/material_48dp"
        android:hint="put something to search"
        android:text="ebod 580" />

    <TextView
        android:id="@+id/comp_magnet_tv_go_search"
        android:layout_width="match_parent"
        android:layout_height="@dimen/material_48dp"
        android:gravity="center"
        android:text="go_search" />

    <View style="@style/DividerLine.Horizontal" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/comp_magnet_tv_get_all"
            android:layout_width="0dp"
            android:layout_height="@dimen/material_48dp"
            android:layout_weight="1"
            android:gravity="center"
            android:text="get all" />


        <TextView
            android:id="@+id/comp_magnet_tv_config_get"
            android:layout_width="0dp"
            android:layout_height="@dimen/material_48dp"
            android:layout_weight="1"
            android:gravity="center"
            android:text="config_get" />

        <TextView
            android:id="@+id/comp_magnet_tv_config_save"
            android:layout_width="0dp"
            android:layout_height="@dimen/material_48dp"
            android:layout_weight="1"
            android:gravity="center"
            android:text="config_save" />

    </LinearLayout>


    <Space
        android:layout_width="wrap_content"
        android:layout_height="@dimen/material_60dp" />
    <TextView
        android:id="@+id/iv_install_plugin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="install plugin" />

    <Space
        android:layout_width="wrap_content"
        android:layout_height="@dimen/material_16dp" />
    <TextView
        android:id="@+id/iv_test_plugin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="test plugin" />

    <Space
        android:layout_width="wrap_content"
        android:layout_height="@dimen/material_16dp" />
    <TextView
        android:id="@+id/iv_test_plugin_java"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="test java plugin" />


    <Space
        android:layout_width="wrap_content"
        android:layout_height="@dimen/material_16dp" />
    <TextView
        android:id="@+id/iv_test_loader_keys"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="iv_test_loader_keys" />

    <Space
        android:layout_width="wrap_content"
        android:layout_height="@dimen/material_16dp" />
    <TextView
        android:id="@+id/iv_test_load_pag1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="iv_test_load_pag1" />

    <Space
        android:layout_width="wrap_content"
        android:layout_height="@dimen/material_16dp" />
    <TextView
        android:id="@+id/iv_test_has_next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="iv_test_has_next" />

    <Space
        android:layout_width="wrap_content"
        android:layout_height="@dimen/material_16dp" />
    <TextView
        android:id="@+id/iv_test_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="iv_test_update" />

    <Space
        android:layout_width="wrap_content"
        android:layout_height="@dimen/material_16dp" />

    <TextView
        android:id="@+id/iv_test_callback"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="iv_test_callback" />


</LinearLayout>